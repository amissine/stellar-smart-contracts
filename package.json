{
  "private": true,
  "name": "stellar-smart-contracts",
  "version": "1.0.0",
  "description": "Stellar Smart Contracts test-driven development",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "ava": "^3.15.0",
    "prettier": "^1.19.1",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "ttab": "^0.7.1"
  },
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "moment": "^2.29.1",
    "node-fetch": "^3.0.0",
    "stellar-sdk": "^8.3.0"
  },
  "scripts": {
    "ava": "export $(cat .env|tr -d [:blank:]|xargs) && ava --verbose --watch test/*.test.mjs",
    "preinit": "npx envdist",
    "init": "export $(cat .env|tr -d [:blank:]|xargs)",
    "prestart": "npm i && npm run init",
    "start": "ttab -w -d $PWD exec npm run www",
    "pretest": "npm i && npm run init",
    "test": "ttab -w -d $PWD exec npm run ava",
    "www": "export $(cat .env|tr -d [:blank:]|xargs) && node --trace-warnings bin/www.mjs"
  },
  "license": "Apache-2.0"
}
